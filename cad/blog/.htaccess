Options +SymLinksIfOwnerMatch
RewriteEngine On

RewriteBase /cad/blog/

RewriteRule ^([0-9a-z_-]+)/([0-9a-z_-]+)/?$ new.php?v1=$1&v2=$2 [QSA,L]
RewriteRule ^([0-9a-z_-]+)/([0-9a-z_-]+)/([0-9a-z_-]+)/?$ new.php?v1=$1&v2=$2&v3=$3 [QSA,L]
RewriteRule ^([0-9a-z_-]+)/([0-9a-z_-]+)/([0-9a-z_-]+)/([0-9a-z_-]+)/?$ new.php?v1=$1&v2=$2&v3=$3&v4=$4 [QSA,L]


